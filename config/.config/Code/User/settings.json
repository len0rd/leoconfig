{
    "files.eol": "\n",
    "editor.fontFamily": "'FiraCode Nerd Font Mono'",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 7,
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.scrollback": 5000,
    "workbench.colorTheme": "Night Owl",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 4,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "workbench.editor.revealIfOpen": true,
    "cmake.configureOnOpen": false,
    "omnisharp.useModernNet": false,
    "redhat.telemetry.enabled": false,
    "xml.format.enforceQuoteStyle": "preferred",
    "docker.languageserver.formatter.ignoreMultilineInstructions": true,
    "security.workspace.trust.untrustedFiles": "open",
    "javascript.format.semicolons": "insert",
    "hediet.vscode-drawio.resizeImages": null,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.location": "top",
    "files.insertFinalNewline": true,
    "cmake.allowUnsupportedPresetsVersions": true,
    "cmake.showOptionsMovedNotification": false,
    "remote.autoForwardPortsSource": "hybrid",
    "window.zoomLevel": 1,
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "cSpell.spellCheckOnlyWorkspaceFiles": true,
    "cSpell.customDictionaries": {},
    "hediet.vscode-drawio.theme": "atlas",
    "restructuredtext.linter.disabledLinters": [
        "doc8"
    ],
    "restructuredtext.linter.run": "off",
    "C_Cpp.clang_format_fallbackStyle": "none",
    "extensions.ignoreRecommendations": true,
    "C_Cpp.formatting": "clangFormat",
    "editor.formatOnSave": true,
    "files.associations": {
        "*.jdebug": "c"
    },
    "git.openRepositoryInParentFolders": "never",
    "git.blame.editorDecoration.enabled": true,
    "git.ignoreMissingGitWarning": true,
}
